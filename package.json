    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "firebase-functions-test": "^0.3.3",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^27.5.1",
    "node-fetch": ">=2.6.7",
